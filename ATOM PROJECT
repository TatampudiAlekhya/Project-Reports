-- Update the rating of a movie
UPDATE Movie
SET rating = 'PG'
WHERE title = 'The Shawshank Redemption';
-- Increment the ticket price by 10% for all tickets
UPDATE Ticket
SET price = price * 1.10;
-- Update the location of a theater
UPDATE Theater
SET location = 'New Location'
WHERE theater_name = 'ABC Theater';
-- Update the role of an actor
UPDATE Actor
SET role = 'Main Character'
WHERE first_name = 'Tom' AND last_name = 'Hardy';
--Update the payment method for a transaction
UPDATE Payment
SET payment_method = 'Credit Card'
WHERE transaction_id = 1001;
-- Modify the datatype of the 'duration' column in the Showing table from TIME to
VARCHAR(50)
ALTER TABLE Showing
MODIFY duration VARCHAR(50);
